{
  "name": "@iodigital/vite-plugin-prism",
  "description": "Prism, OpenAPI mocking server, integration for Vite",
  "version": "0.9.2",
  "author": {
    "name": "iO",
    "website": "https://iodigital.com"
  },
  "repository": "https://github.com/iodigital-com/vite-plugin-prism",
  "keywords": [
